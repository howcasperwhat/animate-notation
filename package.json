{
  "name": "animate-notation",
  "type": "module",
  "version": "0.1.10",
  "packageManager": "pnpm@10.7.0",
  "description": "Create notations and control animations of it.",
  "author": "Casper Huang",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./types": {
      "types": "./build/types/index.d.ts",
      "import": "./build/types/index.js",
      "require": "./build/types/index.cjs"
    },
    "./path-generator": {
      "types": "./build/path-generator/index.d.ts",
      "import": "./build/path-generator/index.js",
      "require": "./build/path-generator/index.cjs"
    },
    "./path-animator": {
      "types": "./build/path-animator/index.d.ts",
      "import": "./build/path-animator/index.js",
      "require": "./build/path-animator/index.cjs"
    }
  },
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest",
    "vue:dev": "npm -C examples/vue run dev",
    "vue:build": "npm -C examples/vue run build",
    "vue:preview": "npm -C examples/vue run preview",
    "react:dev": "npm -C examples/react run dev",
    "react:build": "npm -C examples/react run build",
    "react:preview": "npm -C examples/react run preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jiti": "catalog:",
    "jsdom": "catalog:",
    "roughjs": "catalog:",
    "tsup": "catalog:",
    "vitest": "catalog:"
  }
